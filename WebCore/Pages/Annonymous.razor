@page "/annonymous"

<h1>Annonymous person</h1>
<Serializer ObjectToSerialize="@ToSerialize" />

@code {
public Object ToSerialize { get; private set; }

protected sealed override void OnInitialized()
{
    ToSerialize = new
    {
        FirstName = "My Name",
        MiddleName = default(object),
        LastName = "My last name"
    };

    base.OnInitialized();
}
}
